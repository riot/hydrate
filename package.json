{
  "name": "@riotjs/hydrate",
  "version": "10.0.0",
  "description": "Riot.js hydrate strategy for SSR applications",
  "type": "module",
  "main": "index.js",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "files": [
    "index.cjs",
    "index.js"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm test",
    "lint": "eslint src/*.js test/*.js rollup.config.js",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "npm run lint && mocha -r test/setup.js --exit test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riot/hydrate.git"
  },
  "keywords": [
    "riot",
    "hydrate",
    "ssr",
    "Riot.js",
    "components"
  ],
  "peerDependencies": {
    "riot": "^6.0.0 || ^7.0.0 || ^9.0.0 || ^10.0.0"
  },
  "author": "Gianluca Guarini <gianluca.guarini@gmail.com> (https://gianlucaguarini.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/riot/hydrate/issues"
  },
  "homepage": "https://github.com/riot/hydrate#readme",
  "devDependencies": {
    "@riotjs/compiler": "^10.0.0",
    "@riotjs/prettier-config": "^1.1.0",
    "@riotjs/register": "^10.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "chai": "^6.0.1",
    "eslint": "^9.33.0",
    "eslint-config-riot": "^5.0.2",
    "jsdom": "26.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^11.7.1",
    "prettier": "^3.6.2",
    "riot": "10.0.0",
    "rollup": "^4.47.1",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.1"
  },
  "dependencies": {
    "morphdom": "^2.7.7"
  }
}
